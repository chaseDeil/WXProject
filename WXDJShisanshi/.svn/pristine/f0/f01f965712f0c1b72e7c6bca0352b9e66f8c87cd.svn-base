<!--pages/mine/editProfile.wxml-->
<page>
  <view class="feedback">
    <view class="feedback-content">
      <textarea class="feedback-content-info" bindinput="bindTextAreaChange" placeholder="感谢你提出的宝贵意见，我们会尽快完善" value="{{infoShow}}" maxlength="{{noteMaxLen}}" focus></textarea>
      <view class="number-limit">
        <view class="fr">{{noteNowLen}}/{{noteMaxLen}}</view>
      </view>
    </view>
    <view class="saveBtnView"> 
  <button class="saveBtn"  size="{{primarySize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="logout">保存</button> 
  </view>
  </view>

</page>