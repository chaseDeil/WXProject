.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid #777;
  text-align: center;
  line-height: 80rpx;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 25%;
  color: #777;
}

.on {
  color: #b6011f;
  border-bottom: 5rpx solid #b6011f;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.swiper-box view {
  text-align: center;
}

.notify_view {
  width: 100%;
  background-color: #d4b801;
  color: #b6011f;
  font-size: 30rpx;
  text-align: center;
  padding-bottom: 10rpx;
  padding-top: 10rpx;
}

.divide0 {
  width: 100%;
  height: 2rpx;
  background-color: #f1f1f1;
}

.divide1 {
  width: 100%;
  height: 10rpx;
  background-color: #f7f7f7;
}

.divide2 {
  width: 100%;
  height: 2rpx;
  background-color: #f1f1f1;
  margin-bottom: 10rpx;
}

.item_text1 {
  margin-left: 30rpx;
  color: #a1a1a1;
  width: 100%;
  font-size: 30rpx;
  margin-top: 2rpx;
  margin-bottom: 2rpx;
  display: flex;
}

.item_text2 {
  margin-left: 30rpx;
  margin-bottom: 20rpx;
  color: #a1a1a1;
  width: 100%;
  font-size: 30rpx;
  margin-top: 2rpx;
  display: flex;
}

.item_text_title {
  margin-left: 30rpx;
  margin-top: 6rpx;
  margin-bottom: 2rpx;
  display: flex;
  font-size: 34rpx;
}
